<!doctype html>
<html lang="en">
<head>
    <% include partials/template/head.ejs %>
</head>
<body>
<header id="header">
    <% include partials/template/header.ejs %>
</header>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="section-title" style="margin-top: 120px;">
                <h1>Lease Application</h1>
            </div>
        </div>
    </div>

    <div class="row">
        <form method="post" class="form-horizontal" id="application-form" data-parsley-trigger="keyup">
            <div class="panel panel-default">
                <div class="panel-heading">Rental Details</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="apartment" class="col-sm-4 control-label">Please Select Apartment</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <select class="form-control" id="apartment" name="apartment">
                                            <% apartments.forEach(function(apartment) { %>
                                            <option><%= apartment.address %></option>
                                            <% }); %>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="leaseStart" class="col-sm-6 control-label">Lease term - month to month:
                                    Beginning</label>
                                <div class="col-sm-6">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-calendar-check-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="date" required="" class="form-control" id="leaseStart"
                                               name="leaseStart">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="rent" class="col-sm-6 control-label">Monthly Rent Amount</label>
                                <div class="col-sm-6">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input data-parsley-type="number" required="" id="rent" name="rent"
                                               class="form-control"
                                               placeholder="1200.00">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="securityDeposit" class="col-sm-6 control-label">Security Deposit</label>
                                <div class="col-sm-6">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input data-parsley-type="number" required="" id="securityDeposit"
                                               name="securityDeposit"
                                               class="form-control" placeholder="1200.00">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="keyDeposit" class="col-sm-6 control-label">Key Deposit</label>
                                <div class="col-sm-6">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input data-parsley-type="number" required="" id="keyDeposit" name="keyDeposit"
                                               class="form-control"
                                               placeholder="1200.00">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Applicant Details</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="fullName" class="col-sm-4 control-label">Full Name</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user-o"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" class="form-control" id="fullName" name="fullName"
                                               placeholder="Victor Ayala" required=""
                                               pattern="^[A-Za-z\s]{1,}[\.]{0,1}[A-Za-z\s]{0,}$">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="emailAddress" class="col-sm-4 control-label">Email Address</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-envelope-o"
                                                                          aria-hidden="true"></i></div>
                                        <input data-parsley-type="email" id="emailAddress" name="emailAddress"
                                               class="form-control" placeholder="victor.ayala@gmail.com" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="phoneNumber" class="col-sm-4 control-label">Phone Number</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-mobile"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="phoneNumber" name="phoneNumber" class="form-control"
                                               placeholder="XXX-XX-XXXX" required=""
                                               pattern="/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="socialSecurityNumber" class="col-sm-4 control-label">Social Security
                                    Number</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon">SSN</div>
                                        <input type="text" id="socialSecurityNumber" name="socialSecurityNumber"
                                               class="form-control" placeholder="XXX-XX-XXXX" required=""
                                               pattern="/^\d{3}-\d{2}-\d{4}$/"
                                               data-parsley-error-message="This value should be a valid social security number formatted like : XXX-XX-XXXX ">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="dateOfBirth" class="col-sm-4 control-label">Date of birth</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-calendar-check-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="date" id="dateOfBirth" name="dateOfBirth" class="form-control"
                                               required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="driverLicense" class="col-sm-4 control-label">Driver License
                                    Number</label>
                                <div class="col-sm-8">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-id-card-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="driverLicense" name="driverLicense"
                                               class="form-control" placeholder="XXXX-XXXX-XXXX" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Emergency Contact Information</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="emergencyContactName" class="col-sm-2 control-label">Name</label>
                                <div class="col-sm-10">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="emergencyContactName" name="emergencyContactName"
                                               class="form-control" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="emergencyContactPhoneNumber"
                                       class="col-sm-2 control-label">Phone</label>
                                <div class="col-sm-10">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-mobile"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="emergencyContactPhoneNumber"
                                               name="emergencyContactPhoneNumber" class="form-control" required=""
                                               pattern="/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="emergencyContactAddress" class="col-sm-2 control-label">Address</label>
                                <div class="col-sm-10">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="emergencyContactAddress"
                                               name="emergencyContactAddress" class="form-control"
                                               placeholder="optional">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Residence History</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="currentAddress" name="currentAddress"
                                               class="form-control" placeholder="present full address" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <select class="form-control" id="howLongInCurrentAddress"
                                            name="howLongInCurrentAddress" required="">
                                        <option value="" selected disabled>how long?</option>
                                        <option value=">1"><1 year</option>
                                        <option value="1">1 year</option>
                                        <option value="2">2 years</option>
                                        <option value="3">3 years</option>
                                        <option value="4">4 years</option>
                                        <option value="5">5 years</option>
                                        <option value="6">6 years</option>
                                        <option value="7">7 years</option>
                                        <option value="8">8 years</option>
                                        <option value="9">9 years</option>
                                        <option value="10">10 years</option>
                                        <option value=">10">>10 years</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="number" id="currentAddressRent" name="currentAddressRent"
                                               class="form-control" placeholder="rent amount" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="currentLandlordName" name="currentLandlordName"
                                               placeholder="landlord name" class="form-control" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-mobile"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="landlordNumber" name="landlordNumber"
                                               class="form-control"
                                               placeholder="landlord phone number"
                                               pattern="/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="previousAddress" name="previousAddress"
                                               class="form-control" placeholder="previous full address">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <select class="form-control" id="howLongInPreviousAddress"
                                            name="howLongInPreviousAddress" required="">
                                        <option value="" selected disabled>how long?</option>
                                        <option value=">1"><1 year</option>
                                        <option value="1">1 year</option>
                                        <option value="2">2 years</option>
                                        <option value="3">3 years</option>
                                        <option value="4">4 years</option>
                                        <option value="5">5 years</option>
                                        <option value="6">6 years</option>
                                        <option value="7">7 years</option>
                                        <option value="8">8 years</option>
                                        <option value="9">9 years</option>
                                        <option value="10">10 years</option>
                                        <option value=">10">>10 years</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="text" id="previousAddressRent" name="previousAddressRent"
                                               class="form-control" placeholder="rent amount">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="previousLandlordName" name="previousLandlordName"
                                               placeholder="landlord name" class="form-control">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-mobile"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="previousLandlordNumber" name="previousLandlordNumber"
                                               placeholder="landlord phone number" class="form-control"
                                               pattern="/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Employment Information</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" class="form-control" id="currentEmployerName"
                                               name="currentEmployerName"
                                               placeholder="current employer name"
                                               required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="currentEmployerAddress" name="currentEmployerAddress"
                                               class="form-control" placeholder="employer address" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-phone"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="currentEmployerNumber" name="currentEmployerNumber"
                                               class="form-control" placeholder="employer number" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-black-tie"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="currentPosition" name="currentPosition"
                                               class="form-control" placeholder="position" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-eye" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="currentSupervisorName" name="currentSupervisorName"
                                               class="form-control" placeholder="supervisor name">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <select class="form-control" id="howLongInCurrentPosition"
                                            name="howLongInCurrentPosition" required="">
                                        <option value="" selected disabled>how long?</option>
                                        <option value=">1"><1 year</option>
                                        <option value="1">1 year</option>
                                        <option value="2">2 years</option>
                                        <option value="3">3 years</option>
                                        <option value="4">4 years</option>
                                        <option value="5">5 years</option>
                                        <option value="6">6 years</option>
                                        <option value="7">7 years</option>
                                        <option value="8">8 years</option>
                                        <option value="9">9 years</option>
                                        <option value="10">10 years</option>
                                        <option value=">10">>10 years</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="previousEmployerName" name="previousEmployerName"
                                               class="form-control" placeholder="previous employer name">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-building-o"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="previousEmployerAddress"
                                               name="previousEmployerAddress" class="form-control"
                                               placeholder="employer address">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-phone"
                                                                          aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="previousEmployerNumber" name="previousEmployerNumber"
                                               class="form-control" placeholder="employer number">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-black-tie"
                                                                          aria-hidden="true"></i></div>
                                        <input type="text" id="previousPosition" name="previousPosition"
                                               class="form-control" placeholder="position">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon"><i class="fa fa-eye" aria-hidden="true"></i>
                                        </div>
                                        <input type="text" id="previousSupervisorName" name="previousSupervisorName"
                                               class="form-control" placeholder="supervisor name">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <select class="form-control" id="howLongInPreviousPosition"
                                            name="howLongInPreviousPosition">
                                        <option value="" selected disabled>how long?</option>
                                        <option value=">1"><1 year</option>
                                        <option value="1">1 year</option>
                                        <option value="2">2 years</option>
                                        <option value="3">3 years</option>
                                        <option value="4">4 years</option>
                                        <option value="5">5 years</option>
                                        <option value="6">6 years</option>
                                        <option value="7">7 years</option>
                                        <option value="8">8 years</option>
                                        <option value="9">9 years</option>
                                        <option value="10">10 years</option>
                                        <option value=">10">>10 years</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Personal Finance Information</div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <label for="weeklyIncome" class="control-label">Please provide income information.</label>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="number" id="weeklyIncome" name="weeklyIncome"
                                               class="form-control" placeholder="weekly income" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="number" id="otherSourceOfIncomeAmount"
                                               name="otherSourceOfIncomeAmount"
                                               class="form-control" placeholder="other income" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="number" id="savingsBalance" name="savingsBalance"
                                               class="form-control" placeholder="savings balance" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group">
                                        <div class="input-group-addon">$</div>
                                        <input type="number" id="checkingBalance" name="checkingBalance"
                                               class="form-control" placeholder="checking balance" required="">
                                    </div>
                                </div>
                            </div>
                        </div>
